{
    "family": "lista-certa-task",
    "containerDefinitions": [
        {
            "name": "lista-certa-app",
            "image": "<IMAGE1_NAME>",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": []
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::050451389210:role/ecsTaskExecutionRole2"
}